#include <stdio.h>

int main(void) {
	
	float percent, marksOfSubject1 , marksOfSubject2, marksOfSubject3, marksOfSubject4, marksOfSubject5, total;
	
	printf("Enter marks of subject1: ");
  
	scanf("%f", &marksOfSubject1);
	
	printf("Enter marks of subject2: ");
  
	scanf("%f", &marksOfSubject2);
  
	printf("Enter marks of subject3: ");
  
	scanf("%f", &marksOfSubject3);
  
	printf("Enter marks of subject4: ");
  
	scanf("%f", &marksOfSubject4);
  
	printf("Enter marks of subject5: ");
  
	scanf("%f", &marksOfSubject1);	
	
  total = marksOfSubject1 + marksOfSubject2 + marksOfSubject3 + marksOfSubject4 + marksOfSubject5;
	
  percent = total/5 ;
	
	printf("percent = %f\n", percent);
	
	return 0;
}
